<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zinc.jrecycleview">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".refreshAndLoad.RefreshAndLoadActivity" />
        <activity android:name=".swipe.SwipeActivity" />
        <activity android:name=".anim.AnimActivity" />

        <activity android:name=".stick.content.StickContentActivity" />
        <activity android:name=".stick.header.StickHeaderActivity" />

        <activity android:name=".diy.DiyArticleListActivity" />
        <activity android:name=".mix.MixActivity" />

    </application>

</manifest>